{
  "kind": "collectionType",
  "collectionName": "event_types",
  "info": {
    "singularName": "event-type",
    "pluralName": "event-types",
    "displayName": "Event Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "event_types"
    }
  }
}
